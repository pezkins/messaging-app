{
  "expo": {
    "name": "LinguaLink",
    "slug": "lingualink",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "lingualink",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f172a"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lingualink.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0f172a"
      },
      "package": "com.lingualink.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "apiUrl": "https://aji93f9i0k.execute-api.us-east-1.amazonaws.com/prod",
      "wsUrl": "wss://ksupcb7ucf.execute-api.us-east-1.amazonaws.com/prod",
      "googleClientIdWeb": "75949298562-t5r741c9ud61cnbsrhc95eghutjo5h4m.apps.googleusercontent.com",
      "googleClientIdIos": "75949298562-g1bf5a941mad8hmha4css3ci7fef5q5o.apps.googleusercontent.com",
      "googleClientIdAndroid": "75949298562-73qi3lq9l9kemosrhj0j33sos1mst7l2.apps.googleusercontent.com"
    }
  }
}
