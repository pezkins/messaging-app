{
  "expo": {
    "name": "Intok",
    "slug": "intok",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "intok",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f172a"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.intok.app",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "Intok needs microphone access for voice messages"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0f172a"
      },
      "package": "com.intok.app",
      "versionCode": 1,
      "permissions": [
        "INTERNET",
        "RECORD_AUDIO",
        "VIBRATE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ],
    "sdkVersion": "54.0.0",
    "extra": {
      "apiUrl": "https://aji93f9i0k.execute-api.us-east-1.amazonaws.com/prod",
      "wsUrl": "wss://ksupcb7ucf.execute-api.us-east-1.amazonaws.com/prod",
      "googleClientIdWeb": "75949298562-t5r741c9ud61cnbsrhc95eghutjo5h4m.apps.googleusercontent.com",
      "googleClientIdIos": "75949298562-g1bf5a941mad8hmha4css3ci7fef5q5o.apps.googleusercontent.com",
      "googleClientIdAndroid": "75949298562-73qi3lq9l9kemosrhj0j33sos1mst7l2.apps.googleusercontent.com",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fe7726a8-ed08-49b6-b790-6838da3a61db"
      }
    },
    "owner": "pezkins"
  }
}
